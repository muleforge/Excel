<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.0"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:stdio="http://www.mulesource.org/schema/mule/stdio/2.0"
       xmlns:vm="http://www.mulesource.org/schema/mule/vm/2.0"
       xmlns:xls="http://www.mulesource.org/schema/mule/excel/2.0"
       xsi:schemaLocation="
               http://www.mulesource.org/schema/mule/core/2.0 http://www.mulesource.org/schema/mule/core/2.0/mule.xsd
               http://www.mulesource.org/schema/mule/stdio/2.0 http://www.mulesource.org/schema/mule/stdio/2.0/mule-stdio.xsd
               http://www.mulesource.org/schema/mule/excel/2.0 mule-excel.xsd
               http://www.mulesource.org/schema/mule/vm/2.0 http://www.mulesource.org/schema/mule/vm/2.0/mule-vm.xsd">


    <model name="splitterModel">
    	<!-- Standard simple XLS Row splitter -->
    	<service name="testXlsRowSplitter1">
    		<inbound>
    			<vm:inbound-endpoint address="vm://testXlsSplitter1"/>
    		</inbound>
    		<outbound>
	    		<xls:row-splitter>
	    			<stdio:outbound-endpoint system="OUT"/>
	    		</xls:row-splitter>
    		</outbound>
    	</service>
    	
    	<!-- Standard simple XLS Row splitter, but setting a sheet -->
    	<service name="testXlsRowSplitter2">
    		<inbound>
    			<vm:inbound-endpoint address="vm://testXlsSplitter2"/>
    		</inbound>
    		<outbound>
	    		<xls:row-splitter sheetName="AnotherSheet">
	    			<stdio:outbound-endpoint system="OUT"/>
	    		</xls:row-splitter>
    		</outbound>
    	</service>
   	</model>
</mule>